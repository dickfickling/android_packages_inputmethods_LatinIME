<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin"
>
    <include
        latin:keyboardLayout="@xml/kbd_key_styles" />
    <Row
        latin:keyWidth="10%p"
    >
        <Key
            latin:keyLabel="'"
            latin:keyHintLabel="1"
            latin:moreKeys="1" />
        <include
            latin:keyboardLayout="@xml/kbd_dvorak_f1" />
        <Key
            latin:keyStyle="punctuationKeyStyle" />
        <Key
            latin:keyLabel="p"
            latin:keyHintLabel="4"
            latin:moreKeys="4" />
        <Key
            latin:keyLabel="y"
            latin:keyHintLabel="5"
            latin:moreKeys="5" />
        <Key
            latin:keyLabel="f"
            latin:keyHintLabel="6"
            latin:moreKeys="6" />
        <Key
            latin:keyLabel="g"
            latin:keyHintLabel="7"
            latin:moreKeys="7" />
        <Key
            latin:keyLabel="c"
            latin:keyHintLabel="8"
            latin:moreKeys="8" />
        <Key
            latin:keyLabel="r"
            latin:keyHintLabel="9"
            latin:moreKeys="9" />
        <Key
            latin:keyLabel="l"
            latin:keyHintLabel="0"
            latin:moreKeys="0"
            latin:keyWidth="fillRight" />
    </Row>
    <Row
        latin:keyWidth="10%p"
    >
        <Key
            latin:keyLabel="a" />
        <Key
            latin:keyLabel="o" />
        <Key
            latin:keyLabel="e" />
        <Key
            latin:keyLabel="u" />
        <Key
            latin:keyLabel="i" />
        <Key
            latin:keyLabel="d" />
        <Key
            latin:keyLabel="h" />
        <Key
            latin:keyLabel="t" />
        <Key
            latin:keyLabel="n" />
        <Key
            latin:keyLabel="s"
            latin:keyWidth="fillRight" />
    </Row>
    <Row
        latin:keyWidth="10%p"
    >
        <Key
            latin:keyLabel="q" />
        <Key
            latin:keyLabel="j" />
        <Key
            latin:keyLabel="k" />
        <Key
            latin:keyLabel="x" />
        <Key
            latin:keyLabel="b" />
        <Key
            latin:keyLabel="m" />
        <Key
            latin:keyLabel="w" />
        <Key
            latin:keyLabel="v" />
        <Key
            latin:keyLabel="z" />
        <Key
            latin:keyStyle="deleteKeyStyle"
            latin:keyWidth="fillRight" />
    </Row>
    <Row
		latin:keyWidth="10%p"
    >
	    <Key
		    latin:keyStyle="shiftKeyStyle"
    		latin:keyWidth="15%p" />
    	<Key
    		latin:keyStyle="toSymbolKeyStyle"
    		latin:keyWidth="15%p" />
    	<Key
    		latin:keyStyle="spaceKeyStyle"
    		latin:keyWidth="40%p" />
    	<include
    		latin:keyboardLayout="@xml/kbd_settings_or_tab" />
    	<Key
    		latin:keyStyle="returnKeyStyle"
    		latin:keyWidth="fillRight" />
    </Row>
	
</merge>
